import java.util.*;
class Main{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        String s=sc.nextLine();
        String res="";
        for(int i=0;i<s.length();i++){
            if(s.charAt(i)=='*'){
                continue;
            }
            if(i>0 && s.charAt(i-1)=='*'){
                continue;
            }
            if(i<s.length()-1 && s.charAt(i+1)=='*')
                continue;
            res+=s.charAt(i);
        }
        System.out.println(res);
    }
 }
