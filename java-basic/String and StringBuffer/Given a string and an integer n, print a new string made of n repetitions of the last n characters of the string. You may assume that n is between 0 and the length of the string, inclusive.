import java.util.*;
class Main{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        String s=sc.nextLine();
        String res="";
        int n=sc.nextInt();
        for(int i=0;i<n;i++){
            res+=s.substring(s.length()-n,s.length());
        }
        System.out.println(res);
    }
}
