import java.util.*;
class Main{
public static void main(String args[]){
    Scanner sc=new Scanner(System.in);
    System.out.print("Enter size of array:");
    int n=sc.nextInt();
    int a[]=new int[n];
    System.out.println("Enter array elements:");
    for(int i=0;i<n;i++){
        a[i]=sc.nextInt();
    }
    int max=a[0],min=a[0];
    for(int i=0;i<n;i++){
       if(a[i]>max)
        max=a[i];

       if(a[i]<min)
        min=a[i];
    }
    System.out.print("Maximum Element:"+max+"\nMinimum Element:"+min);
}
}
