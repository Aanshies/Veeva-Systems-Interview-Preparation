import java.util.*;
class Main{
    
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        char n=sc.next().charAt(0);
        char m=sc.next().charAt(0);
        if(n < m){System.out.println(n+" , "+m);}
        else{System.out.println(m+" , "+n);}
        }
    }
