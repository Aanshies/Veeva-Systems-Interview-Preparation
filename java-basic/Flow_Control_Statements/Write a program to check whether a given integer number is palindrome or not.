import java.util.*;

class Main {
    public static void main(String args[]) {
        System.out.print("Enter a number:");
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt() , rev=0;
        int temp=n;
        while(n!=0){
            rev=rev*10+(n%10);
            n=n/10;
        }
        System.out.print(temp==rev?"Palindrome":"Not a Palindrome");
            }
        }
