import java.util.*;

class Main {
    public static void main(String args[]) {
        System.out.print("Enter a number:");
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt(), sum=0, rev=0;;
        int temp=n;
        while(n!=0){
            int digit=n%10;
            sum=sum+digit;
            n=n/10;
        }
        int temp1=sum;
        while(sum!=0){
            rev= rev*10+(sum%10);
            sum=sum/10;
        }
        int mul=temp1*rev;
        System.out.print(mul==temp?"Magical number":"Not a Magical number");
            }
        }
